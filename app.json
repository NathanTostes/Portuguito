{
  "expo": {
    "name": "Portuguito",
    "slug": "Portuguito",
    "version": "4.0.0",
    "orientation": "portrait",
    "icon": "./src/Imagens/icons/portuguitaBase.jpeg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Portuguito"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconePortuguito4.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.Portuguito",
      "versionCode": 11
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f77b7b16-b39c-4f91-9aaf-904fc7400136"
      }
    }
  }
}
